{
  "name": "@prettier/plugin-ruby",
  "version": "0.18.0",
  "description": "prettier plugin for the Ruby programming language",
  "main": "src/ruby.js",
  "scripts": {
    "build": "node-gyp rebuild",
    "clean": "node-gyp clean",
    "lint": "eslint --cache .",
    "print": "prettier --plugin=.",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prettier/plugin-ruby.git"
  },
  "author": "Kevin Deisz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prettier/plugin-ruby/issues"
  },
  "homepage": "https://github.com/prettier/plugin-ruby#readme",
  "dependencies": {
    "prettier": ">=1.10"
  },
  "devDependencies": {
    "all-contributors-cli": "^6.12.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "jest": "^25.1.0",
    "node-gyp": "^6.1.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "env": {
      "es6": true,
      "jest": true,
      "node": true
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ]
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./test/js/setupTests.js"
    ],
    "testRegex": ".test.js$"
  },
  "prettier": {
    "trailingComma": "none"
  }
}
